<h1> INDEXXXXXXXXXXXXX</h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th>No.</th>
      <th>Title</th>
      <th>View</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% @boards.each do |board|%>
    <tr>
      <td><%= board.id%></td>
      <td><%= board.title%></td>
      <td><a href="/boards/<%=board.id%>">Show</a></td>
      <td><a href="/boards/<%=board.id%>/edit">Edit</a></td>
      <td><a href="/boards/<%=board.id%>" data-method="delete" data-confirm="Are U Sure?">Delete</a></td>
    </tr>
    <% end %>
  </tbody>
</table>
<a href="/boards/new">New</a>
